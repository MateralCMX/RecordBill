<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">起始日期</view>
    </view>
    <view class="weui-cell__bd">
      <picker mode="date" value="{{SearchM.minDate}}" bindchange="bindMinDateChange">
        <view class="weui-input">{{SearchM.minDateStr}}</view>
      </picker>
    </view>
  </view>
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__hd">
      <view class="weui-label">结束日期</view>
    </view>
    <view class="weui-cell__bd">
      <picker mode="date" value="{{SearchM.maxDate}}" bindchange="bindMaxDateChange">
        <view class="weui-input">{{SearchM.maxDateStr}}</view>
      </picker>
    </view>
  </view>
</view>
<view class="weui-btn-area">
  <button class="weui-btn" type="primary" bindtap="Search">查询</button>
</view>